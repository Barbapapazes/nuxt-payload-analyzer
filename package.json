{
  "name": "nuxt-payload-analyzer",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@8.7.1",
  "description": "Analyze Nuxt Payload Size",
  "author": "Est√©ban Soubiran <e.soubiran25@gmail.com> (https://github.com/barbapapazes/)",
  "license": "MIT",
  "homepage": "https://github.com/barbapapazes/nuxt-payload-analyzer",
  "keywords": [
    "nuxt",
    "cli",
    "payload",
    "analyzer"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "nuxt-payload-analyzer": "./bin/nuxt-payload-analyzer.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev:generate": "nuxt generate playground",
    "nuxt-payload-analyzer": "node bin/nuxt-payload-analyzer.mjs --cwd playground",
    "build": "unbuild",
    "build:stub": "unbuild --stub",
    "release": "bumpp",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "citty": "^0.1.4",
    "consola": "^3.2.3",
    "pathe": "^1.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "1.0.0-beta.22",
    "@types/node": "^20.8.7",
    "bumpp": "^9.2.0",
    "eslint": "^8.51.0",
    "jiti": "^1.20.0",
    "nuxt": "^3.8.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0"
  }
}
